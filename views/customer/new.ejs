<form action="/customer/create" method="post">
  <h2>Create a Customer</h2>

  <input type="text" placeholder="Name" name="name"><br/>
  <input type="text" placeholder="Email" name="email"><br />
  <input type="text" placeholder="Home Phone" name="homephone"><br />
  <input type="text" placeholder="Cell Phone" name="cellphone"><br />
  <input type="text" placeholder="Street Address" name="street">
  <input type="text" placeholder="City" name="city">
  <input type="text" placeholder="State" name="state">
  <input type="number" placeholder="Zip" name="zip">

  <input type="submit" value="Create Customer"/>
</form>
