<form action="/stock/update/<%= stock.id %>" method="POST" >
  <h2>Update Stock</h2>

  <input value="<%= stock.symbol %>" type="text" name="symbol" ><br />
  <input value="<%= stock.name %>" type="text" name="name" ><br />
  <input value="<%= stock.number_of_shares %>" type="text" name="number of shares"  />
  <input value="<%= stock.purchase_price %>" type="text" name="purchase price" />
  <input value="<%= stock.date %>" type="text" name="date" />

  <input type="submit" value="Edit Stock" />
</form>
